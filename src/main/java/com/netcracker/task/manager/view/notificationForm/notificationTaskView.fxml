<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="145.0" prefWidth="338.0" xmlns="http://javafx.com/javafx/11.0.1"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.netcracker.task.manager.view.notificationForm.NotificationTaskController">
    <Button fx:id="buttonPostpone" layoutX="123.0" layoutY="105.0" mnemonicParsing="false"
            onAction="#clickButtonPostponed" prefHeight="25.0" prefWidth="92.0" text="Postpone"/>
    <Button fx:id="buttonCancel" layoutX="238.0" layoutY="105.0" mnemonicParsing="false"
            onAction="#clickButtonCancelled" prefHeight="25.0" prefWidth="85.0" text="Cancel"/>
    <Label fx:id="labelTaskName" layoutY="-6.0" prefHeight="32.0" prefWidth="341.0"
           style="-fx-background-color: #FFBB40;" textAlignment="CENTER">
        <padding>
          <Insets left="2.0" right="2.0"/>
        </padding>
    </Label>
    <Button fx:id="buttonDone" layoutX="14.0" layoutY="105.0" mnemonicParsing="false" onAction="#clickButtonDone"
            prefHeight="25.0" prefWidth="92.0" text="Done"/>
    <Label fx:id="labelTaskDescription" layoutX="-3.0" layoutY="36.0" prefHeight="38.0" prefWidth="341.0"
           textAlignment="CENTER">
        <padding>
          <Insets left="5.0" right="5.0"/>
        </padding>
    </Label>
    <Label fx:id="labelTime" layoutX="-1.0" layoutY="74.0" prefHeight="17.0" prefWidth="341.0" textAlignment="CENTER">
        <padding>
          <Insets left="5.0" right="5.0"/>
        </padding>
    </Label>
</AnchorPane>
